	Keywords = {Super-Kamiokande, Photomultiplier tube readout, Charge-to-time converter, Self-trigger, high-speed, Low-noise},
	Number = {3},
	Pages = {710},
	Title = {High-speed charge-to-time converter ASIC for the Super-Kamiokande detector},
