	Doi = {https://doi.org/10.1016/S0927-6505(97)00012-1},
	Issn = {0927-6505},
	Journal = {Astropart. Phys.},
	Number = {1},
	Pages = {125},
	Title = {Evolution of the cosmic gas and the relic supernova neutrino background},
	Url = {https://www.sciencedirect.com/science/article/pii/S0927650597000121},
	Volume = {7},
	Year = {1997},
	Bdsk-Url-1 = {https://www.sciencedirect.com/science/article/pii/S0927650597000121},
