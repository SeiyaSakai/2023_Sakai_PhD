	Journal = {Mon. Not. R. Astron. Soc.},
	Month = {12},
	Number = {1},
	Pages = {1363},
	Title = {{Diffuse supernova neutrino background from extensive core-collapse simulations of 8--100 M⊙ progenitors}},
