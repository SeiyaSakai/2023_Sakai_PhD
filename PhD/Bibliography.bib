@article{1990Burrows,
	Abstract = {More than half a century ago, in a prescient paper, Baade and Zwicky (1934) first connected neutron star formation with supernovae. To these authors there seemed to be a natural correspondence between the large binding energies of neutron stars (GM2/R {\textasciitilde} 1053 ergs), whose existence had only just recently been postulated (Landau, 1932), and the large explosion energies that seemed to be associated with the newly identified supernovae. Such were the ambiguities in supernova light curves, spectra, and distances and such was the novelty of the new supernova concept that as much as 1053 ergs in supernova light and debris kinetic energy seemed perfectly consistent with the observations. The binding energy that must be shed to form the ultracompact neutron star, naturally, was to escape as light and kinetic energy in some combination. Indeed, how else?},
	Address = {New York, NY},
	Author = {Burrows, Adam S.},
	Booktitle = {Supernovae},
